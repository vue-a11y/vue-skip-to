{
  "name": "@vue-a11y/skip-to",
  "version": "1.0.6",
  "description": "It helps people who only use the keyboard to jump to what matters most.",
  "main": "dist/vue-skip-to.umd.js",
  "module": "dist/vue-skip-to.esm.js",
  "unpkg": "dist/vue-skip-to.min.js",
