{
  "name": "@vue-a11y/skip-to",
  "version": "3.0.0",
  "description": "It helps people who only use the keyboard to jump to what matters most.",
  "main": "dist/skip-to.cjs.js",
  "style": "dist/index.css",
  "browser": "dist/skip-to.es.js",
  "module": "dist/skip-to.es.js",
  "unpkg": "dist/skip-to.iife.js",
  "jsdelivr": "dist/skip-to.iife.js",
  "exports": {
    ".": {
      "import": "./dist/skip-to.es.js",
      "require": "./dist/skip-to.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all -s clean build:*",
    "build:vite": "vite build",
    "demo:vite": "cd demo-vite && vite",
    "release": "standard-version",
    "clean": "rm -rf dist",
    "test:unit": "jest",
    "test:e2e": "node_modules/.bin/cypress run --headless",
    "test:e2e:open": "node_modules/.bin/cypress open",
    "project:publish": "git push --follow-tags origin master && npm run build && npm publish --access public --tag next"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-a11y/vue-skip-to.git"
  },
  "keywords": [
    "a11y",
    "accessibility",
    "skip to content",
    "vue.js",
    "keyboard",
    "skip links",
    "vuepress",
    "visual-impairments",
    "screen-reader",
    "tab",
    "keyboard"
  ],
  "author": "Alan Ktquez <ktquez@gmail.com> (https://github.com/ktquez)",
  "contributors": [
    {
      "name": "William Lindvall",
      "email": "w-lindvall@outlook.com",
      "url": "https://github.com/shwilliam"
    },
    {
      "name": "Marcus Herrmann",
      "email": "github@marcus-herrmann.com",
      "url": "https://github.com/marcus-herrmann"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-a11y/vue-skip-to/issues"
  },
  "homepage": "https://github.com/vue-a11y/vue-skip-to#readme",
  "dependencies": {
    "vue": "^3.0.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/test-utils": "^1.0.2",
    "@vuepress/plugin-register-components": "^1.4.1",
    "@vuepress/theme-default": "^1.4.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "chokidar": "^3.4.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.5.0",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^7.23.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.8.0",
    "eslint-plugin-vuejs-accessibility": "^0.6.1",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "standard-version": "^8.0.0",
    "vite": "^2.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.4.1"
  }
}
